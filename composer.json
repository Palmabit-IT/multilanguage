    "description": "This package helps handling of language and translation with laravel framework",
    "license" : "mit",
    "authors": [
        {
            "name": "jacopo beschi",
            "email": "j.beschi@palmabit.com",
            "homepage": "http://www.jacopobeschi.com"
        },
        {
