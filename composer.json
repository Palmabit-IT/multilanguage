    "autoload": {
        "psr-0": {
            "Palmabit\\Multilanguage\\": "src/Palmabit/Multilanguage"
        },
        "psr-4": {
            "Palmabit\\Multilanguage\\Tests\\" : "tests/"
        },
        "files": [
            "src/Palmabit/Multilanguage/Helper/FormHelper.php"
        ],
        "classmap": [
            "src/controllers"
        ]
