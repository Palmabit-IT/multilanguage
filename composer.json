    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
        },
    "require-dev": {
        "mockery/mockery": "0.9.0",
        "orchestra/testbench": "2.2.*",
        "doctrine/dbal": "2.4.*",
        "fzaninotto/faker": "1.4.*"
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "git://github.com/orchestral/phpseclib.git"
    }],
    "autoload": {
        "psr-4": {
            "Palmabit\\Multilanguage\\": "src/Palmabit/Multilanguage",
            "Palmabit\\Multilanguage\\Tests\\" : "tests/"
        },
        "classmap": [
            "src/controllers",
            "src/commands"
        ]
    },
    "minimum-stability": "stable"
